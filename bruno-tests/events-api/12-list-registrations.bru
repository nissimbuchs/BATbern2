meta {
  name: List Event Registrations
  type: http
  seq: 12
}

get {
  url: {{baseUrl}}/events/{{createdEventId}}/registrations
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.status: eq 200
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return registrations list", function() {
    const response = res.getBody();
    expect(response.data).to.be.an('array');
    expect(response.pagination).to.be.an('object');
  });
}
